<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="com.niuxin.mapper.TemplateMapper">
     <resultMap type="com.niuxin.bean.Template"  id="r">
        
    </resultMap>
    <parameterMap type="com.niuxin.bean.Template"  id="p"></parameterMap>
    

    
    
      <insert id="insert" parameterMap="p" useGeneratedKeys="true" keyProperty="id">    
        insert into template(contract,operation,price,handnum,position,minnum,maxnum,remark,pictureurl,audiourl,sendto,createtime,updatetime,audioread,sendfrom,occupy,name) 
        values(#{contract},#{operation},#{price},#{handnum},#{position},#{minnum},#{maxnum},#{remark},#{pictureurl},#{audiourl},#{sendto},#{createtime},#{updatetime},#{audioread},#{sendfrom},#{occupy},#{name})
    </insert>
 
     <select id="selectAll" resultMap="r">    
        select * template form order by id asc
    </select>
    
      <select id="selectById" resultMap="r" parameterType="int">    
        select * template form where id= #{id}
    </select>
    
    
     <update id="update" parameterMap="p">  
        update template set name=#{name},updatetime=#{updateTime} where id=#{id}
    </update>
    
    
         <delete id="delete" parameterType="int">  
        delete from template where id=#{id}
    </delete>
    
    
    
    
    
</mapper>